package interfaces;

import java.util.EventListener;
/**
 * Interface pour les ecouteurs du choix du terrain
 * @author Andy Lam
 *
 */

public interface ChoixTerrainListener extends EventListener{
	/**
	 * Ecoute quelle terrain a ete choisi
	 * @param choixTerrain le nom du terrain choisi
	 */
	public void terrainChange(String choixTerrain);
}
